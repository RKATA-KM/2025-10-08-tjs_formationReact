{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "uicmp":{
      "path": "src/components/ui",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"grc_templates/uitemplate/TemplateName.tsx"
      }
    },
    "layoutcmp":{
      "path": "src/components/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"grc_templates/layoutTemplate/TemplateName/TemplateName.tsx",
        "test":"grc_templates/layoutTemplate/TemplateName/TemplateName.test.tsx",
        "story":"grc_templates/layoutTemplate/TemplateName/TemplateName.stories.tsx"
      }
    }
  }
}